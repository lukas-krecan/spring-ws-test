<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://www.mycompany.com/schema/myns"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:beans="http://www.springframework.org/schema/beans"
    targetNamespace="http://net.javacrumbs/schema/spring-ws-test"
    elementFormDefault="qualified"
    attributeFormDefault="unqualified" xmlns:Q1="http://net.javacrumbs/schema/spring-ws-test">

   <xsd:import namespace="http://www.springframework.org/schema/beans"/>

   <xsd:element name="mock-message-sender">
      <xsd:complexType>
   		<xsd:attribute name="id" type="xsd:ID"/>
      </xsd:complexType>
   </xsd:element>
   
   <xsd:element name="response-generator">
      <xsd:complexType>
  			<xsd:sequence>
  				<xsd:element name="namespaces" type="xsd:string" minOccurs="0"/>
  				<xsd:element name="xpaths"	minOccurs="0" type="xsd:string"/>
  			</xsd:sequence>
  			<xsd:attribute name="id" type="xsd:ID"/>
      </xsd:complexType>
   </xsd:element>
   
   <xsd:element name="compare-request-validator">
      <xsd:complexType>
  			<xsd:sequence>
  				<xsd:element name="namespaces" type="xsd:string" minOccurs="0"/>
  				<xsd:element name="xpaths"	minOccurs="0" type="xsd:string"/>
  			</xsd:sequence>
  			<xsd:attribute name="id" type="xsd:ID"/>
      </xsd:complexType>
   </xsd:element>



</xsd:schema>